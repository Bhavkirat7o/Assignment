#include <iostream>
#include <string>

int main() {
    // Biodata variables
    std::string name = "Bhvakirat Singh";
    std::string uid = "22BCS10189";
    std::string section = "630";
    std::string areaOfInterest = "Coding and Gymnasium";
    int ageLastYear = 20; // Assuming you were 20 last year
    float cgpa = 6.6;
    std::string hometown = "Jammu";

    // Print biodata
    std::cout << "Full Name: " << name << std::endl;
    std::cout << "UID: " << uid << std::endl;
    std::cout << "Section: " << section << std::endl;
    std::cout << "Area of Interest: " << areaOfInterest << std::endl;
    std::cout << "Age Last Year: " << ageLastYear << std::endl;
    std::cout << "CGPA: " << cgpa << std::endl;
    std::cout << "Hometown: " << hometown << std::endl;

    return 0;
}
